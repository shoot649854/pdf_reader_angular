{
  "entry": ["src/**/*.ts", "src/**/*.tsx", "public/**"],
  "project": "tsconfig.json",
  "ignore": [
    "node_modules",
    "postcss.config.js",
    "postcss.config.mjs",
    "eslint.config.mjs",
    ".eslintrc.json",
    "public/**"
  ],
  "ignoreBinaries": ["only-allow", "poetry", "jest"],
  "ignoreDependencies": [
    "hidden-package",
    "Unlisted binaries",
    "raw-loader",
    "@angular-eslint/template-parser"
  ]
}
